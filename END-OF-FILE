import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int a=1;
        while(sc.hasNextLine()){
            System.out.println(a+" "+sc.nextLine());
            a++;
        }
        sc.close();
    }
}
